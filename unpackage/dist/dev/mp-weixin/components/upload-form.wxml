<view class="upload-wrapper"><block wx:if="{{type==='batch'}}"><block wx:for="{{uploadList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="upload-form van-hairline--surround"><view class="upload-label van-hairline--bottom">{{item.label}}</view><view class="upload-content"><view data-event-opts="{{[['tap',[['handleUpload',['$0'],[[['uploadList','',index]]]]]]]}}" class="upload-button" bindtap="__e">导入CSV</view><view class="upload-file"><view class="upload-file-name van-ellipsis">{{item.fileName}}</view><view data-event-opts="{{[['tap',[['handleCancelFile',['$0'],[[['uploadList','',index]]]]]]]}}" hidden="{{!(item.fileName)}}" class="upload-cancel" bindtap="__e"><van-icon vue-id="{{'344f6afe-1-'+index}}" name="clear" size="20px" data-com-type="wx" bind:__l="__l"></van-icon></view></view></view></view></block></block><block wx:else><view class="upload-form van-hairline--surround"><view class="upload-label van-hairline--bottom">{{uploadList[0].label}}</view><view class="upload-content-batch"><view class="upload-content--left"><view data-event-opts="{{[['tap',[['handleUploadBatch',['$event']]]]]}}" class="upload-button" bindtap="__e">批量导入CSV</view><view data-event-opts="{{[['tap',[['handleClearBatch',['$event']]]]]}}" hidden="{{!(uploadList[0].uploadFiles.length>0)}}" class="upload-button" bindtap="__e">清空导入</view></view><view class="upload-content--right"><block wx:for="{{uploadList[0].uploadFiles}}" wx:for-item="file" wx:for-index="index" wx:key="index"><view class="upload-file"><view class="upload-file-name van-ellipsis">{{file.fileName}}</view><view data-event-opts="{{[['tap',[['handleCancelFile',['$0',index],[[['uploadList.__$n0.uploadFiles','',index]]]]]]]}}" hidden="{{!(file.fileName)}}" class="upload-cancle" bindtap="__e"><van-icon vue-id="{{'344f6afe-2-'+index}}" name="clear" size="20px" data-com-type="wx" bind:__l="__l"></van-icon></view></view></block></view></view></view></block></view>